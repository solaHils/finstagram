<h2>New Post</h2>
<ul class="errors">
  <% @cutestagram_post.errors.full_messages.each do |message| %>
    <li><%= message %></li>
  <% end %>
</ul>

<form method="POST" action="/cutestagram_posts">
    <div class="form-group">
        <label for="photo_url">Photo URL</label>
        <input type="text" name="photo_url" id="photo_url" placeholder="Type Photo URL here">
    </div>
    <div class="form-group">
        <button class="button" type="submit">Post</button>
    </div>
</form>